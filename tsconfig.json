{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@/assets/*": ["assets/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
